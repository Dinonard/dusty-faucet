{
    "name": "discord-bot",
    "version": "1.0.0",
    "description": "Discord faucet bot made with discord.js",
    "private": true,
    "keywords": [
        "starter",
        "typescript",
        "discord",
        "bot"
    ],
    "homepage": "https://github.com/PlasmNetwork/dusty-faucet",
    "bugs": {
        "url": "https://github.com/PlasmNetwork/dusty-faucetissues"
    },
    "author": {
        "name": "Kentaro Vadney",
        "url": "https://github.com/VadneyK"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/PlasmNetwork/dusty-faucet.git"
    },
    "main": "build/index.js",
    "type": "module",
    "scripts": {
        "start": "yarn run serve",
        "serve": "cd build && yarn run build && NODE_ENV=production node -r dotenv/config ./index.js",
        "dev": "ts-node-dev -r dotenv/config src/index.ts",
        "build": "tsc --project tsconfig.json",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "lint:check": "eslint '*/**/*.{js,ts,tsx}'",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --coverage",
        "sandbox": "ts-node -r dotenv/config scripts/sandbox.ts"
    },
    "engines": {
        "node": ">=16.x",
        "yarn": ">=1.22.10"
    },
    "license": "MIT",
    "devDependencies": {
        "@jest/types": "^27.0.2",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.18.0",
        "@typescript-eslint/parser": "^4.18.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^27.0.4",
        "jest-config": "^26.6.3",
        "nock": "^13.0.11",
        "prettier": "^2.2.1",
        "ts-jest": "^27.0.3",
        "ts-node": "^9.1.1",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.2.3"
    },
    "dependencies": {
        "@plasm/types": "^1.5.4-3",
        "@polkadot/api": "^4.15.1",
        "@polkadot/api-contract": "^4.15.1",
        "@polkadot/keyring": "^6.9.1",
        "@polkadot/types": "^4.15.1",
        "@polkadot/util": "^6.9.1",
        "@polkadot/util-crypto": "^6.9.1",
        "discord.js": "^12.5.1",
        "dotenv": "^8.0.0",
        "trim-newlines": "^4.0.1"
    }
}
